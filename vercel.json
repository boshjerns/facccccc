{
    "version": 2,
    "builds": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
          }          
    ],
    "routes": [
      {
        "src": "/api/v3/text2img",
        "methods": ["POST"],
        "dest": "/server.js"
      },
      {
        "src": "/api/v3/fetch",
        "methods": ["POST"],
        "dest": "/server.js"
      }
    ]
  }
  